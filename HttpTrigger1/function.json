{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "inputBlob",
      "type": "blob",
      "path": "incontainer/{data}.jpg",
      "direction": "in"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "path": "outcontainer/{data}-Copy.jpg",
      "direction": "out"
    }
  ]
}
